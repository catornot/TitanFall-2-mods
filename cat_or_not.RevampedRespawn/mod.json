{
	"Name" : "cat_or_not.RevampedRespawn",
	"Description" : "Spawn ticks on death and respawns player with ships",
	"Version": "1.1.0",
	"LoadPriority": 1,
	"Scripts": [
		{
			"Path": "sh_TICKSettings.gnut",
			"RunOn": "MP",
			"ServerCallback": {
				"After": "TICK_Init"
			},
			"ClientCallback": {
				"After": "TICK_Init"
			}
		},
		{
			"Path": "_widow_utility.nut",
			"RunOn": "SERVER && MP"
		},
		{
			"Path": "_fd_dropship_utility.nut",
			"RunOn": "SERVER && MP"
		},
		{
			"Path": "_dropship_drivable.nut",
			"RunOn": "SERVER && MP"
		},
		{
			"Path": "precache_some_stuff.nut",
			"RunOn": "(CLIENT || SERVER) && MP",
			"ServerCallback": {
				"After": "PrecacheStuff"
			},
			"ClientCallback": {
				"After": "PrecacheStuff"
			}
		}
	],
	"Localisation": [
		"resources/tick_localisation_%language%.txt"
	]
}

